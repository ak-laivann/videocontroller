{
  "version": "1.0.0",
  "manifest_version": 3,
  "name": "Video Controller",
  "description": "Video Controller Chrome Extension built using react with typescript",
  "action": {
    "default_title": "Video Controller",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["contentScript.js"]
    }
  ]
}
